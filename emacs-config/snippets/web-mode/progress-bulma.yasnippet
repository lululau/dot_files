# -*- mode: snippet -*-
# name: bulma-progress-bar
# key: progress
# group: bulma
# binding: direct-keybinding
# --
<progress class="progress is-${1:$$(yas-choose-value '("primary" "link" "success" "info" "warning" "danger" "link" "white" "light" "dark" "black" "text" "ghost"))}" value="$2" max="100$3">$4%</progress>$0